<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tubestba">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SET_ALARM" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.TubesTBA">
        <activity
            android:name=".HintActivity"
            android:theme="@style/Theme.AppCompat.PopMe" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RankingActivity" />
        <activity
            android:name=".PopLogin"
            android:theme="@style/Theme.AppCompat.PopMe" />
        <activity
            android:name=".SalahActivity"
            android:theme="@style/Theme.AppCompat.PopMe" />
        <activity
            android:name=".BenarActivity"
            android:theme="@style/Theme.AppCompat.PopMe" />
        <activity android:name=".GameActivity" />
        <activity android:name=".LevelActivity" />
        <activity
            android:name=".PopSetting"
            android:theme="@style/Theme.AppCompat.PopMe" />
        <activity
            android:name=".PopCaraBermain"
            android:theme="@style/Theme.AppCompat.PopMe" />
        <activity android:name=".MainActivity" />
        <receiver android:name=".NotificationReceiver" />
    </application>

</manifest>